  "dependencies": {
    "ws": "^8.16.0",
    "readline": "^1.3.0",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.1",
    "inquirer": "^9.2.12",
    "axios": "^1.6.2",
    "form-data": "^4.0.0",
    "sharp": "^0.33.0",
    "node-fetch": "^3.3.2"
